FUNC testpcall ( testsys ) $[
    programcall -to @p -as400 @@testsys -program /QSYS.LIB/UBLUTEST.LIB/SIMPLPROG0.PGM
    put -to ~ TEST0901D programcall -to @p -as400 @@testsys -in ~ 10 CHAR  -program /QSYS.LIB/UBLUTEST.LIB/SIMPLPROG1.PGM
    put @p
    put -to @foo ENOD put -to @bar RES
    put @p
    put -to ~ TEST0901D programcall -to @p -as400 @@testsys -in ~ 10 CHAR -in @foo 4 CHAR -program /QSYS.LIB/UBLUTEST.LIB/SIMPLPROG2.PGM
    put @p
    # put -to ~ TEST0901D programcall -to @p -as400 @@testsys -in ~ 10 CHAR -in @foo 4 CHAR -out @bar 4 CHAR -program /QSYS.LIB/UBLUTEST.LIB/SIMPLPROG2.PGM
]$
