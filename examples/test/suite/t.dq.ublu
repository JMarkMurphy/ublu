# Assumes called from test.all.ublu so
# ... so defaults are already included
#
# include -s test_utils.ublu
# ... is already included from test.all.ublu
#
# Assumes the present file is residing in examples/test/suite 
include ../../../extensions/ux.dqentry.ublu

# t.dq ( work_dir testsys testrptpf )
# ... work_dir - where tests can create files and leave them
# ... testsys - the server against which tests to be run
# ... testrptpf - open pf to write test report records
FUNC t.dq ( work_dir testsys testrptpf ) $[
    LOCAL @FILNAM LOCAL @TMSTRT  LOCAL @TMEND LOCAL @DESC LOCAL @PFAIL
    LOCAL @result
    LOCAL @qifqp LOCAL @qs LOCAL @qk
    LOCAL @expect LOCAL @output LOCAL @dqe
    LOCAL @outfile
    LOCAL @qdesc
    LOCAL @key LOCAL @bkey LOCAL @bdata
    LOCAL @kqname
    
    put ${ ---------- }$
    put ${ Start t.dq }$
    put ${ ---------- }$
    put ${ }$
    
    fqp_obj ( *t_library *t_dataq DTAQ )
    tuple -assign @qifqp ~
    
    cat_fp ( @@work_dir t.dq.miscout.txt )
    put -to @outfile ~
    
    map -to @result
    
    string -to @FILNAM -trim t.dq.ublu
    @result -> @FILNAM
    
    string -to @qdesc -trim ${ test sequential queue }$
    
    string -to @DESC -trim ${ seq queue:create and exists: }$
    @DESC -to @DESC -cat @qifqp
    @result -> @DESC
    
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    @result -> @TMSTRT
        
    dq -to @qs -as400 @@testsys @qifqp
    @qs -desc @qdesc -create 1024
    @qs -to @output -exists
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    @result -> @TMEND
    
    IF @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    @result -> @PFAIL
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )
    
    string -to @DESC -trim ${ seq queue:write and peek }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    put -to @expect ${ hello sequential queue }$
    @qs -write @expect
    @qs -to @dqe -peek
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getString ( @dqe )
    string -to ~ -eq ~ @expect
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$    
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )

    string -to @DESC -trim ${ seq queue:read }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @dqe -read
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getString ( @dqe )
    string -to ~ -eq ~ @expect
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )
    
    string -to @DESC -trim ${ seq queue:query ccsid }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query ccsid
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    eval -to ~ == @output 0
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result ) 

    string -to @DESC -trim ${ seq queue:query description }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query description

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to @output -trim
    
    @output -to ~ -eq @qdesc
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result ) 
    
    string -to @DESC -trim ${ seq queue:query fifo }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query fifo

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL

    writeMapToRecord ( @@testrptpf @result ) 
    
    string -to @DESC -trim ${ seq queue:query forceauxstorage }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query forceauxstorage

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )     

    string -to @DESC -trim ${ seq queue:query maxentrylength }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query maxentrylength

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    eval -to ~ == @output 1024
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )     
 
    string -to @DESC -trim ${ seq queue:query name }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query name

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to ~ -eq *t_dataq
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )  
    
    string -to @DESC -trim ${ seq queue:query path }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query path

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to ~ -eq @qifqp
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )
    
    string -to @DESC -trim ${ seq queue:query savesender }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -to @output -query savesender

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )     

    string -to @DESC -trim ${ seq queue:delete and exists }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qs -delete  
    @qs -to @output -exists
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$    
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )
    
    string -to @qdesc -trim ${ test keyed queue }$
    
    string -to @kqname -cat *t_dataq K
    
    fqp_obj ( *t_library @kqname DTAQ )
    tuple -assign @qifqp ~

    string -to @DESC -trim ${ keyed queue:create and exists: }$
    @DESC -to @DESC -cat @qifqp
    
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
        
    dq -to @qk -as400 @@testsys -keyed @true @qifqp
    @qk -desc @qdesc -keylen 4 -create 1024
    @qk -to @output -exists
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )

    string -to @DESC -trim ${ keyed queue:write and peek }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    put -to @expect ${ hello keyed queue }$
    @qk -key abcd -write @expect
    @qk -to @dqe -key abcd -peek
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getString ( @dqe )
    string -to ~ -eq ~ @expect
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$    
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )

    string -to @DESC -trim ${ keyed queue:read }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @dqe -key abcd -read
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getString ( @dqe )
    string -to ~ -eq ~ @expect
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL

    string -to @DESC -trim ${ keyed queue:bwrite and bpeek }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    put -to @expect ${ hello bkeyed queue }$
    string -to @bkey -tobytes efgh
    @expect -to @bdata -tobytes

    @qk -bkey @bkey -writeb @bdata
    @qk -to @dqe -bkey @bkey -peek
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getData ( @dqe )
    calljava -to ~ -class java.util.Arrays -method equals -primarg ~ -primarg @bdata
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$    
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )

    string -to @DESC -trim ${ keyed queue:bread }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
      
    @qk -to @dqe -bkey @bkey -read
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    ux.dqe.getData ( @dqe )
    calljava -to ~ -class java.util.Arrays -method equals -primarg ~ -primarg @bdata
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )

    string -to @DESC -trim ${ keyed queue:query keylen }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query keylen
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    eval -to ~ == @output 4
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result ) 
    string -to @DESC -trim ${ keyed queue:query ccsid }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query ccsid
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    eval -to ~ == @output 0
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result ) 

    string -to @DESC -trim ${ keyed queue:query description }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query description

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to @output -trim
    
    @output -to ~ -eq @qdesc
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result ) 
    
    string -to @DESC -trim ${ keyed queue:query fifo }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query fifo

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL

    writeMapToRecord ( @@testrptpf @result ) 
    
    string -to @DESC -trim ${ keyed queue:query forceauxstorage }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query forceauxstorage

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
        
    put @PFAIL
  
    writeMapToRecord ( @@testrptpf @result )     

    string -to @DESC -trim ${ keyed queue:query maxentrylength }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query maxentrylength

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    eval -to ~ == @output 1024
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )     
 
    string -to @DESC -trim ${ keyed queue:query name }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query name

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to ~ -eq @kqname
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )  
    
    string -to @DESC -trim ${ keyed queue:query path }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query path

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    @output -to ~ -eq @qifqp
    IF ~ THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )
    
    string -to @DESC -trim ${ keyed queue:query savesender }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -to @output -query savesender

    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$
    
    put @PFAIL
      
    writeMapToRecord ( @@testrptpf @result )     

    string -to @DESC -trim ${ keyed queue:delete and exists }$
    put -n -s ${ *** }$ put -n -s @DESC
    
    currentTimeMillis ( )
    tuple -assign @TMSTRT ~
    
    @qk -delete  
    @qk -to @output -exists
    
    currentTimeMillis ( )
    tuple -assign @TMEND ~
    
    IF -! @output THEN $[ put -to @PFAIL P ]$ ELSE $[ put -to @PFAIL F ]$    
    
    put @PFAIL
    
    writeMapToRecord ( @@testrptpf @result )    
    
    put ${ ------------ }$    
    put ${ end t.dq }$
    put ${ ------------ }$
    put ${ }$
    
]$

# @qk -clear
